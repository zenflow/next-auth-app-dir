{
  "name": "next-auth-app-dir",
  "version": "0.1.0",
  "description": "Extension of NextAuth.js to play nice with Next.js App Directory",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zenflow/next-auth-app-dir.git"
  },
  "bugs": {
    "url": "https://github.com/zenflow/next-auth-app-dir/issues"
  },
  "author": {
    "name": "Matthew Francis Brunetti",
    "email": "zenflow87@gmail.com"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint-fix": "yarn lint --fix",
    "build": "shx rm -rf dist && tsc",
    "build-watch": "yarn build && tsc --watch",
    "app": "next build test/app --no-lint && next start test/app",
    "app-dev": "next dev test/app"
  },
  "dependencies": {},
  "peerDependencies": {
    "next": "13.0.x",
    "next-auth": "^4.15.1"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.27.0",
    "eslint-config-zenflow": "4.0.0",
    "next": "13.0.2",
    "next-auth": "4.16.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shx": "^0.3.4",
    "typescript": "4.8.4"
  }
}
